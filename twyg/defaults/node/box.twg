@include    "common"

-- 3D orientation of the box.
-- Possible values: topleft, topright, bottomleft, bottomright
boxOrientation          topright

-- 3D depth of the box.
boxDepth                20 * pow(0.7, depth)

-- Color of the horizontal side of the 3D box.
horizSideColor          baseColor.lighten(0.34)

-- Color of the vertical side of the 3D box.
vertSideColor           baseColor.lighten(0.12)

-- Stroke color of the box.
strokeColor             baseColor

